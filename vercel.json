{
    "version": 2,
    "builds": [
      {
        "src": "./src/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/v1/api/(.*)",
        "dest": "./src/index.js",
        "methods": ["GET"],
        "headers": {
            "Access-Control-Allow-Origin": "*"
        }
      }
    ]
  }